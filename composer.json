{
    "name": "cerpus/license-client",
    "description": "Use the Cerpus License server to manage license for content",
    "keywords": ["laravel", "Cerpus License-Server Client"],
    "type": "library",
    "license": "Closed",
    "authors": [
        {
            "name": "Odd-Arne Johansen",
            "email": "odd-arne.johansen@cerpus.com"
        },
        {
            "name": "Thomas Horn Sivertsen",
            "email": "thomass@cerpus.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "cerpus/cerpushelper": "^1.4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Cerpus\\LicenseClient\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.cerpus.net/"
        },
        {
            "type": "composer",
            "url": "https://composer-3rdparty.cerpus.net/"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Cerpus\\LicenseClient\\Providers\\LicenseClientProvider"
            ],
            "aliases": {
                "LicenseClient": "Cerpus\\LicenseClient\\LicenseClient"
            }
        }
    }
}
